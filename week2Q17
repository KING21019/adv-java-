// File: package1/A.java
package package1;

public class A {
    public void display() {
        System.out.println("Class A from package1");
    }
}


// File: package2/B.java
package package2;
import package1.A;

public class B {
    public static void main(String[] args) {
        A obj = new A();
        obj.display();
    }
}
