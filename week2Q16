class ModifierDemo {
    private int privateVar = 10;
    public int publicVar = 20;
    protected int protectedVar = 30;
    int defaultVar = 40;

    private void privateMethod() { System.out.println("Private Method"); }
    public void publicMethod() { System.out.println("Public Method"); }
    protected void protectedMethod() { System.out.println("Protected Method"); }
    void defaultMethod() { System.out.println("Default Method"); }
}

class TestModifiers extends ModifierDemo {
    public static void main(String[] args) {
        TestModifiers t = new TestModifiers();
        System.out.println("Public Var: " + t.publicVar);
        System.out.println("Protected Var: " + t.protectedVar);
        System.out.println("Default Var: " + t.defaultVar);

        t.publicMethod();
        t.protectedMethod();
        t.defaultMethod();
    }
}
